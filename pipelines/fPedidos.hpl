    <limit>0</limit>
    <sql>with produto_transform as (
     select 
	        s.data,
            s.idpedido,
            s.faturamento,
            s.custo,
			produto || ' [' || sabor || ']' AS produto_sabor,
			cast(split_part(qtd, ' ', 1) as int) as qtd,
			split_part(qtd, ' ', 2) as unid
	from stg_basefaturamento s
)
SELECT 
    pt.data,
    pt.idpedido,
    pt.faturamento,
    pt.custo,
    p.idprodutos,
    pt.qtd,
	pt.unid
FROM 
    produto_transform pt
JOIN 
    gold_dprodutos p 
ON 
    pt.produto_sabor = p.produto_sabor</sql>
